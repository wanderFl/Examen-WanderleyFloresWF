<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Examen_WanderleyFloresWF.ViewsWF.segunPaginaWF"
             Title="segunPaginaWF">
    <StackLayout>
        <Label Text="segunda noticia" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" />
        <ListView x:Name="miListView">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <ViewCell>
                        <StackLayout>
                            <Label Text="{Binding paginaWF}" />
                            
                        </StackLayout>
                    </ViewCell>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>